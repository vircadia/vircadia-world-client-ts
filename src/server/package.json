{
  "name": "vircadia-world-server",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsx watch server",
    "build": "esbuild server --bundle --packages=external --platform=node --format=esm --outdir=dist --sourcemap",
    "setup-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-setup.ts",
    "resync-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-resync.ts",
    "check-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-checkStatus.ts",
    "restart-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-restart.ts",
    "stop-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-stop.ts",
    "start-supabase": "tsx ./modules/supabase/supabase-scripts/supabase-start.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.0",
    "peer": "^1.0.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "@types/socket.io": "^3.0.2",
    "esbuild": "^0.21.0",
    "tsx": "^4.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
